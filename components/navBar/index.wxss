.navbar{
  position: relative
}
.home-icon{
  /* width: 28px; */
  height: 100%;
  position: absolute;
  transform: translateY(-50%);
  top: 70%;
  left: 40rpx;
  display: flex;
}
.home-icon image{
  height: 55rpx;
  width: 60rpx;
}
.left-icon{
  /* width: 28px; */
  height: 100%;
  position: absolute;
  transform: translateY(-50%);
  top: 57%;
  left: 40rpx;
  display: flex;
}

.left-icon .image{
  width: 28px;
  height: 33px;
  margin: auto;
}
.nav-title, .nav-icon{
  position: absolute;
  transform: translate(-50%, -50%);
  left: 50%;
  top: 50%;
  font-weight: bold;
}